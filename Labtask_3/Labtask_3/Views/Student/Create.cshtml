@model Labtask_3.Models.Database.Student
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_studentlayout.cshtml";
}
<h1>Create Student</h1>
<form action="/Student/Create" method="post" style="margin:50px; padding:5px;">
    <input type="text" name="name" class="form-control" placeholder="Name" /><br />
    <input type="text" name="dob" class="form-control" placeholder="Date of Birth" /><br />
    <input type="text" name="cgpa" class="form-control" placeholder="Cgpa" /><br />
    <input type="submit" class="btn" />
</form>

