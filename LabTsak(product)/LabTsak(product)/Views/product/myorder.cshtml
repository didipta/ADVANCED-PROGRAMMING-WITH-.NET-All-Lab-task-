
@{
    ViewBag.Title = "myorder";
    Layout = "~/Views/Shared/_pagelayout.cshtml";
}

<h2>myorder</h2>
@if (Model != null)
{
    <table class="table table-bordered">
        <tr>

            <th>Product Name</th>
            <th>Product Price</th>
            <th>Product Qty</th>
            <th></th>
            <th></th>
        </tr>
        @foreach (var b in Model)
        {
            <tr>
                <td>@b.pname</td>
                <td>@b.pprice</td>
                <td>@b.pqty</td>
            </tr>

        }


    </table>
    <form action="/product/Orderproduct" method="post">
        <input type="submit" value="Checkout" class="btn btn-success" />
    </form>
}
else
{
    <p>Product not add in the cart</p>
}



