
@{
    ViewBag.Title = "Myallorder";
    Layout = "~/Views/Shared/_pagelayout.cshtml";
}

<h2>My all order</h2>
<table class="table table-bordered">
    <tr>

        <th>Order id</th>
        <th>Total order</th>
        <th>Order time</th>
        <th></th>
        <th></th>
    </tr>
    @foreach (var b in Model)
    {
        <tr style="padding:50px;">
            <td>@b.Order_id</td>
            <td>@b.total_order</td>
            <td>@b.order_time</td>
            <td><a href="/product/Showdetails/@b.Order_id" class="btn btn-success">Order Detail</a></td>
        </tr>
        }
 </table>
